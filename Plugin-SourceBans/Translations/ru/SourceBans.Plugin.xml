<?xml version="1.0" encoding="utf-8" ?>
<Phrases>
  <Phrase Name="Addban Description">
    <ru>Добавляет игрока в список банов SourceBans</ru>
  </Phrase>
  <Phrase Name="Ban Description">
    <ru>Забанить игрока с сервера</ru>
